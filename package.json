{
  "name": "web_scraping",
  "version": "0.0.1",
  "description": "Web Scraping with Puppeteer",
  "main": "src/index.ts",
  "author": {
    "email": "meelight12@gmail.com",
    "name": "Moises Reyes",
    "url": "https://github.com/MeeLight"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "format": "prettier . --write"
  },
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "puppeteer": "20.9.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/morgan": "1.9.4",
    "@types/node": "20.4.4",
    "morgan": "1.10.0",
    "prettier": "3.0.0",
    "ts-node-dev": "2.0.0",
    "typescript": "5.1.6"
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          "./puppeteer.config.cjs",
          "./src/**/*.ts"
        ],
        "options": {
          "tabWidth": 2,
          "embeddedLanguageFormatting": "auto",
          "endOfLine": "auto",
          "singleQuote": true,
          "useTabs": false,
          "semi": false,
          "trailingComma": "none",
          "singleAttributePerLine": false,
          "arrowParens": "avoid",
          "bracketSpacing": true,
          "quoteProps": "as-needed",
          "bracketSameLine": false,
          "printWidth": 80
        }
      }
    ]
  }
}
